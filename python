//1 //8kyu //Basic Mathematical Operations

def basic_op(operator, value1, value2):
    if operator == '+':
        return value1 + value2
    elif operator == '-':
        return value1 - value2
    elif operator == '*':
        return value1 * value2
    else: 
        return value1 / value2
       
//2nd solution
def basic_op(operator, val1, val2):
    return {'+':val1+val2,'-':val1-val2,'*':val1*val2,'/':val1/val2}.get(operator)
        
